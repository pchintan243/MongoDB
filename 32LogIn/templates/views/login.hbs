<!doctype html>
<html lang="en">

<head>
    {{>header}}
    <title>Complaint Form</title>
</head>

<body>
    {{>navbar}}
    <div class="container mt-5">
        <form class="row g-3 needs-validation" action="/login" method="post">

            <div class="col-md-7">
                <label for="email" class="form-label">Email:</label>
                <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                    name="Email">
            </div>
            <div class="col-md-3">
                <label for="inputPassword4" class="form-label">Password</label>
                <input type="password" class="form-control" id="inputPassword4" name="Password" autocomplete="on">
            </div>
            <div class="col-12">
                <button class="btn btn-primary m-1" type="submit" name="button">Submit</button>
            </div>
        </form>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
</body>

</html>